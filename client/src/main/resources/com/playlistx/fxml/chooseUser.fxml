<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane prefHeight="200.0" prefWidth="410.0" style="-fx-background-color: transparent;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Pane prefHeight="200.0" prefWidth="410.0" style="-fx-background-radius: 25px;" styleClass="background" />
      <TextField fx:id="chosenUser" layoutX="20.0" layoutY="69.0" prefHeight="27.0" prefWidth="243.0" promptText="Available Users" styleClass="loginField" />
      <Text fill="WHITE" layoutX="27.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="label" text="Choose a user:">
         <font>
            <Font name="Monospaced Bold" size="28.0" />
         </font>
      </Text>
      <Button layoutX="315.0" layoutY="156.0" mnemonicParsing="false" onAction="#confirm" styleClass="loginButton" text="Confirm" />
      <Button layoutX="254.0" layoutY="156.0" mnemonicParsing="false" onAction="#cancel" styleClass="loginButton" text="Cancel" />
   </children>
</AnchorPane>
