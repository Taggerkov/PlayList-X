<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<HBox alignment="CENTER" style="-fx-background-color: -secondaryColor; -fx-background-radius: 25;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" maxWidth="1.7976931348623157E308" minWidth="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
         <children>
            <Label style="-fx-background-radius: 10px;" text="Title">
               <font>
                  <Font size="10.0" />
               </font>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </Label>
            <Label fx:id="songTitle" style="-fx-background-radius: 10px;" text="&quot;">
               <font>
                  <Font size="14.0" />
               </font>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </Label>
         </children>
      </VBox>
      <VBox alignment="CENTER" maxHeight="-Infinity" minWidth="30.0" prefWidth="50.0" HBox.hgrow="SOMETIMES">
         <children>
            <Label style="-fx-background-radius: 10px;" text="Year">
               <font>
                  <Font size="10.0" />
               </font>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </Label>
            <Label fx:id="songYear" style="-fx-background-radius: 10px;" text="00">
               <font>
                  <Font size="14.0" />
               </font>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </Label>
         </children>
      </VBox>
      <VBox alignment="CENTER" maxWidth="1.7976931348623157E308" minWidth="-Infinity" prefWidth="75.0" HBox.hgrow="ALWAYS">
         <children>
            <Label style="-fx-background-radius: 10px;" text="Artist">
               <font>
                  <Font size="10.0" />
               </font>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </Label>
            <Label fx:id="songArtist" style="-fx-background-radius: 10px;" text="&quot;">
               <font>
                  <Font size="14.0" />
               </font>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </Label>
         </children>
      </VBox>
      <VBox alignment="CENTER" prefWidth="100.0" HBox.hgrow="ALWAYS">
         <children>
            <Label style="-fx-background-radius: 10px;" text="Genre">
               <font>
                  <Font size="10.0" />
               </font>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </Label>
            <Label fx:id="songGenre" style="-fx-background-radius: 10px;" text="&quot;">
               <font>
                  <Font size="14.0" />
               </font>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </Label>
         </children>
      </VBox>
      <VBox alignment="CENTER" prefWidth="100.0" HBox.hgrow="ALWAYS">
         <children>
            <Label style="-fx-background-radius: 10px;" text="Album">
               <font>
                  <Font size="10.0" />
               </font>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </Label>
            <Label fx:id="songAlbum" style="-fx-background-radius: 10px;" text="&quot;">
               <font>
                  <Font size="14.0" />
               </font>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </Label>
         </children>
      </VBox>
      <VBox alignment="CENTER" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="100.0" HBox.hgrow="ALWAYS">
         <children>
            <Label style="-fx-background-radius: 10px;" text="Duration">
               <font>
                  <Font size="10.0" />
               </font>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </Label>
            <Label fx:id="songDuration" style="-fx-background-radius: 10px;" text="&quot;">
               <font>
                  <Font size="14.0" />
               </font>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </Label>
         </children>
      </VBox>
   </children>
</HBox>
